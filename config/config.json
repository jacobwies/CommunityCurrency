{
    "Web": {
            "S3BucketName": "community-currency-web",
            "S3BucketPolicy": {
                "Version": "2012-10-17",
                "Statement": [
                    {
                        "Sid": "PublicReadGetObject",
                        "Effect": "Allow",
                        "Principal": "*",
                        "Action": "s3:GetObject",
                        "Resource": "arn:aws:s3:::community-currency-web/*"
                    }
                ]
            },
            "S3StaticWebConfiguration": {
                "IndexDocument": {
                "Suffix": "index.html"
            }
        }
    }
}